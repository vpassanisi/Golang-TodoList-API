{
  "name": "todolist",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "npm-run-all -p serve:css serve:js",
    "build": "npm-run-all build:css build:js",
    "lint": "vue-cli-service lint",
    "serve:js": "vue-cli-service serve --port 5000",
    "build:js": "vue-cli-service build",
    "serve:css": "postcss src/css/tailwind.src.css -o src/css/tailwind.css -w",
    "build:css": "postcss src/css/tailwind.src.css -o src/css/tailwind.css --env production"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.4.0",
    "@vue/cli-plugin-eslint": "^4.4.0",
    "@vue/cli-service": "^5.0.8",
    "autoprefixer": "^9.8.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "tailwindcss": "^1.4.6",
    "tailwindcss-bg-alpha": "^2.1.0",
    "tailwindcss-dark-mode": "^1.1.4",
    "vue-fragment": "^1.5.1",
    "vue-mq": "^1.0.1",
    "vue-router": "^3.3.2",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.4.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
